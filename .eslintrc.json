{
  "extends": ["airbnb-base"],
  "rules": {
    "no-unused-vars": "warn",
    "linebreak-style": "off",
    "import/newline-after-import": "warn",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off"
  }
}
